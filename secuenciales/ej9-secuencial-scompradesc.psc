Algoritmo compradesc
	//inicio
	Definir prventa, descuento, totalapagar como real;
	Escribir " Introduce el precio de venta";
	Leer prventa;
	descuento=prventa*0.15;
	totalapagar=prventa-descuento;
	Escribir " El descuento total es: ", descuento, " El precio total de compra es : ", totalapagar;
	FinAlgoritmo
